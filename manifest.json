{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com.ua/"
            ],
            "js": [
                "content.js"
            ],
            "css": ["style.bundle.css", "content.css"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "logo.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}